{
    "parserOptions":  {
                          "ecmaVersion":  "latest",
                          "sourceType":  "module"
                      },
    "settings":  {
                     "react":  {
                                   "version":  "detect"
                               }
                 },
    "extends":  [
                    "eslint:recommended",
                    "plugin:react/recommended",
                    "plugin:react/jsx-runtime"
                ],
    "plugins":  [
                    "react"
                ],
    "rules":  {
                  "react/prop-types":  "off"
              },
    "env":  {
                "browser":  true,
                "es2021":  true
            }
}
